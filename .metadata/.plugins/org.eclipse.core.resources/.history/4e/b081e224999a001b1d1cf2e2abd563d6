package com.java.abs;

public class MainTest {
	
	public static void main(String[] args) {
		Animal dog = new Dog();
		Animal cat = new Cat();
		
		methodRun(dog);
		methodRun(cat);
		
		
	}
	
	public static void methodRun(Animal ani) {
		
		if(ani instanceof Dog){
			ani.run();
		}
		
		if(ani instanceof Cat) {
			ani.run();
		}
		
	}
	
	
}
