package com.java.ex;

public class Ex2 {

	public static void main(String[] args) {
		
		
		int[] intSort = {30,34,11,3,5,-1};
		int tmp = 0;

		for (int i = 0; i < intSort.length; i++) {
			
			for (int j = 0; j < intSort.length; j++) {
				
				if(intSort[i] > intSort[j]) {
					tmp = intSort[i];
					intSort[i] = intSort[j];
					intSort[j] = tmp;
				}
			}
		}

	}

}
